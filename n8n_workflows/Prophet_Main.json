    {
      "parameters": {
        "method": "POST",
        "url": "http://host.docker.internal:11434/api/chat",
        "sendBody": true,
        "specifyBody": "json",
        "jsonBody": "={{ JSON.stringify({\n  model: 'gpt-oss:20b',\n  messages: [\n    {\n      role: 'system',\n      content: `You are a data requirements analyzer. Given a question and already collected data, determine what additional data is needed. Respond in JSON format:\n{\n  \"needs_characters\": true/false,\n  \"needs_rules\": true/false,\n  \"needs_history\": true/false,\n  \"needs_lore\": true/false,\n  \"ready_to_answer\": true/false,\n  \"reasoning\": \"explanation\"\n}`\n    },\n    {\n      role: 'user',\n      content: `Question: ${$json.question}\\n\\nAlready collected: ${JSON.stringify($json.collected_data, null, 2)}`\n    }\n  ],\n  stream: false,\n  options: {\n    temperature: 0.2,\n    num_predict: 200\n  }\n}) }}",
        "options": {
          "timeout": 30000
        }
      },
      "id": "e7f8a9b0-c1d2-3456-0123-567890123456",
      "name": "Analyze Data Needs",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 4.2,
      "position": [
        400,
        800
      ]
    },
    {
      "parameters": {
        "jsCode": "// Merge all collected data from different paths\nconst allItems = $input.all();\nconst firstItem = $input.first().json;\n\nlet collectedData = firstItem.collected_data || {};\nlet references = firstItem.references || [];\n\n// Merge data from MongoDB queries\nfor (const item of allItems) {\n  const data = item.json.data;\n  const refs = item.json.references || [];\n  const queryType = item.json.query_type;\n\n  if (data && queryType) {\n    if (queryType === 'character') {\n      collectedData.characters = data;\n    } else if (queryType === 'turn') {\n      collectedData.turns = data;\n    } else if (queryType === 'campaign') {\n      collectedData.campaign = data[0] || null;\n    }\n  }\n\n  // Merge chunks from Qdrant\n  if (item.json.chunks) {\n    collectedData.rules_chunks = collectedData.rules_chunks || [];\n    collectedData.rules_chunks.push(...item.json.chunks);\n  }\n\n  // Merge references\n  references.push(...refs);\n}\n\n// Deduplicate references\nreferences = [...new Set(references)];\n\nreturn [\n  {\n    json: {\n      ...firstItem,\n      collected_data: collectedData,\n      references: references\n    }\n  }\n];"
      },

      "parameters": {
        "jsCode": "// Extract intent from LLM response\nconst ollamaResponse = $json.message?.content || $json.response || '';\nconst intent = ollamaResponse.trim().toUpperCase();\n\n// Valid intents\nconst validIntents = ['RULES_LOOKUP', 'CHARACTER_INFO', 'GAME_HISTORY', 'DICE_CALCULATION', 'LORE_KNOWLEDGE', 'MULTI_STEP'];\n\n// Default to MULTI_STEP if unrecognized\nconst finalIntent = validIntents.includes(intent) ? intent : 'MULTI_STEP';\n\n// Carry forward all context\nconst previousData = $input.first().json;\n\nreturn [\n  {\n    json: {\n      ...previousData,\n      intent: finalIntent\n    }\n  }\n];"
      },
